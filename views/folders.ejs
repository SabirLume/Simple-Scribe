<section>
<% if (documents.length > 0) { %>
    <% for(var i = 0; i < documents.length; i++) { %>
    <ul class="foldersUl">
        <li>
            <span class="documents"><%= documents[i].title %></span>
        </li> 
    </ul>
    <% } %>
<% } %>
</section>
<section><a href="/my-notes"><button class="btn btn-default">Create Folder</button></a></section>

<script src="main.js"></script>
</body>
</html>